{
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "auto",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^components/(.*)$",
    "^components/UI/(.*)$",
    "^pages/(.*)$",
    "^layouts/(.*)$",
    "^providers/(.*)$",
    "^store/(.*)$",
    "^api/(.*)$",
    "^hooks/(.*)$",
    "^types/(.*)$",
    "^utils/(.*)$",
    "^config/(.*)$",
    "^assets/(.*)$",
    "^constants/(.*)$",
    "^(?!.*[.]scss$)[./].*$",
    "",
    ".scss$"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.0.0"
}
